generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model expenses {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  amount   Int
  rate     Int?
  currency String
  date     DateTime @db.Date
  detail   String
  type     String
}
